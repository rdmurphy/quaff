{
  "name": "quaff",
  "version": "4.1.0",
  "description": "Collect JSON/YAML/YML/CSV/TSV files from a source folder and convert them into a single object.",
  "main": "index.js",
  "types": "types.d.ts",
  "repository": "rdmurphy/quaff",
  "author": "Ryan Murphy <ryan@rdmurphy.org>",
  "license": "MIT",
  "files": [
    "index.js",
    "types.d.ts"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "git-pre-commit": "precise-commits",
    "test": "mocha",
    "coverage": "nyc --reporter=lcov npm run test",
    "release": "np --no-yarn"
  },
  "keywords": [
