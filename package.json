{
  "name": "quaff",
  "version": "4.1.0",
  "description": "Collect JSON/YAML/YML/CSV/TSV files from a source folder and convert them into a single object.",
  "main": "index.js",
  "types": "types.d.ts",
  "repository": "rdmurphy/quaff",
  "author": "Ryan Murphy <ryan@rdmurphy.org>",
  "license": "MIT",
  "files": [
    "index.js",
    "types.d.ts"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "git-pre-commit": "precise-commits",
    "test": "mocha",
    "coverage": "nyc --reporter=html --reporter=text mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "release": "np --no-yarn"
  },
  "keywords": [
    "data",
    "javascript",
    "json",
    "yaml",
    "yml",
    "csv",
    "tsv"
  ],
  "devDependencies": {
    "@types/node": "^13.1.0",
    "@zeit/git-hooks": "^0.1.4",
    "assert-rejects": "^1.0.0",
    "coveralls": "^3.0.3",
    "mocha": "^7.0.0",
    "mocha-lcov-reporter": "^1.0.0",
    "np": "*",
    "nyc": "^15.0.0",
    "precise-commits": "^1.0.2",
    "prettier": "^2.0.1"
  },
  "dependencies": {
    "archieml": "^0.4.2",
    "d3-dsv": "^1.1.1",
    "dset": "^2.0.1",
    "js-yaml": "^3.12.2",
    "parse-json": "^5.0.0",
    "tiny-glob": "^0.2.6"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
