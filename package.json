    "mocha-lcov-reporter": "^1.0.0",
    "np": "^6.2.1",
    "nyc": "^15.0.1",
    "precise-commits": "^1.0.2",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@newswire/prettier-config": "^3.0.0",
    "archieml": "^0.4.2",
    "d3-dsv": "^1.1.1",
    "dset": "^2.0.1",
    "js-yaml": "^3.12.2",
    "parse-json": "^5.0.0",
    "tiny-glob": "^0.2.6"
